{
    "routes": [
      {
        "route": "*",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/images/*.{png,jpg,gif,jpeg}", "/css/*"]
      },
      "responseOverrides": {
        "404": {
          "rewrite": "/404.html"
        }
      }
  }
